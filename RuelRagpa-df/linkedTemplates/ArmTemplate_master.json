{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "ruelragpa-df"
		},
		"AdventureWorks2022_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "uelness.database.windows.net"
		},
		"AdventureWorks2022_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "AdventureWorks2022"
		},
		"AdventureWorksDW2022_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "uelness.database.windows.net"
		},
		"AdventureWorksDW2022_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "AdventureWorksDW2022"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "RuelRagpa-df_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AdventureWorks2022_properties_typeProperties_server": {
						"value": "[parameters('AdventureWorks2022_properties_typeProperties_server')]"
					},
					"AdventureWorks2022_properties_typeProperties_database": {
						"value": "[parameters('AdventureWorks2022_properties_typeProperties_database')]"
					},
					"AdventureWorksDW2022_properties_typeProperties_server": {
						"value": "[parameters('AdventureWorksDW2022_properties_typeProperties_server')]"
					},
					"AdventureWorksDW2022_properties_typeProperties_database": {
						"value": "[parameters('AdventureWorksDW2022_properties_typeProperties_database')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}