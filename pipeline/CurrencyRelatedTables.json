{
	"name": "CurrencyRelatedTables",
	"properties": {
		"activities": [
			{
				"name": "Copy Currency Data to Staging",
				"type": "Copy",
				"dependsOn": [],
				"policy": {
					"timeout": "0.12:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
