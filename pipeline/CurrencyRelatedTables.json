									"type": "Int32",
									"physicalType": "int"
								},
								"sink": {
									"name": "CurrencyKey",
									"type": "Int32",
									"physicalType": "int"
								}
							},
							{
								"source": {
									"name": "DateKey",
									"type": "Int32",
									"physicalType": "int"
								},
								"sink": {
									"name": "DateKey",
									"type": "Int32",
									"physicalType": "int"
								}
							},
							{
								"source": {
									"name": "AverageRate",
									"type": "Double",
									"physicalType": "float"
								},
								"sink": {
									"name": "AverageRate",
									"type": "Double",
									"physicalType": "float"
								}
							},
							{
								"source": {
									"name": "EndOfDayRate",
									"type": "Double",
									"physicalType": "float"
								},
								"sink": {
									"name": "EndOfDayRate",
									"type": "Double",
									"physicalType": "float"
								}
							},
							{
								"source": {
									"name": "Date",
									"type": "DateTime",
									"physicalType": "datetime"
								},
								"sink": {
									"name": "Date",
									"type": "DateTime",
									"physicalType": "datetime"
								}
							}
						],
						"typeConversion": true,
						"typeConversionSettings": {
							"allowDataTruncation": true,
							"treatBooleanAsNumber": false
						}
					}
				},
				"inputs": [
					{
						"referenceName": "FactCurrencyRateStaging",
						"type": "DatasetReference"
					}
				],
				"outputs": [
