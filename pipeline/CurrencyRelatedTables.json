					"source": {
						"type": "AzureSqlSource",
						"sqlReaderQuery": "SELECT\r\n    [CurrencyAlternateKey] = C.CurrencyCode,\r\n    [CurrencyName] = C.[Name]\r\nFROM\r\n    Sales.Currency C\r\nORDER BY\r\n    C.[Name]",
						"queryTimeout": "02:00:00",
						"partitionOption": "None"
					},
					"sink": {
						"type": "AzureSqlSink",
